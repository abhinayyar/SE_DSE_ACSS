<h1> Admin View - Add rounds and round data</h1>
<%= form_for :round, url: rounds_path do |f| %>
  <p>
    <%= f.label :round_name %><br>
    <%= f.text_field :round_name %>
  </p>
 
  <p>
    <%= f.label :round_des %><br>
    <%= f.text_area :round_des %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', rounds_path %>

